{% extends 'base.html.twig' %}

{% block title %}Structure{% endblock %}

{% block body %}
    <h1>Structure</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ structure.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ structure.name }}</td>
            </tr>
            <tr>
                <th>Sell_drink</th>
                <td>{{ structure.sellDrink ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_structure_index') }}">back to list</a>

    <a href="{{ path('app_admin_structure_edit', {'id': structure.id}) }}">edit</a>

    {{ include('admin_structure/_delete_form.html.twig') }}
{% endblock %}
